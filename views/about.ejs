<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>About me</title>
    </head>
    <body>
        <h1>About me</h1>
        <p>Name: <%= name %></p>
        <p>e-mail: <%= email %></p>
        <p>contact phone: <%= phone %></p>
        <h3>Skills</h3>
        <ul>
            <% skills.forEach(function(skill){ %>
                <li>
                    <%= skill %>
                </li>
            <% }) %>
        </ul>
    </body>
</html>